Content-Type: text/ecl
Entry-Name: .content

get-button "Nouveau (Ctrl+N)" | click
with [get-window Nouveau] {
    get-tree | select "Projet Agile Risk Manager"
    get-button "Suivant >" | click
}
with [get-window "Projets Agile Risk Manager"] {
    get-group "Dépôt où créer le projet" | get-table | select "Dépôt local"
    get-editbox -after [get-label "Nom du projet :"] | set-text LeTravail
    get-button "Suivant >" | click
    with [get-section "Bases de connaissances" | get-tree] {
        select "Bases additionnelles"
        get-item -path "Bases additionnelles" | check
        get-item -path "EBIOS Risk Manager" | check
        get-item -path "Bases additionnelles/Critères de sécurité EBIOS 2010" | uncheck
        get-item -path "Bases additionnelles/Cyber kill chain Lockheed Martin" | uncheck
        get-item -path "EBIOS Risk Manager/Cyber kill chain" | uncheck
        get-item -path "EBIOS Risk Manager/Échelle d'activité" | uncheck
        get-item -path "EBIOS Risk Manager/Échelle d'état d'application" | uncheck
    }
    get-button "Suivant >" | click
    get-button | click
    get-button -after [get-label X -after [get-label X 
        -after [get-label "Être en conformité avec les référentiels de sécurité numérique"]]] | click
    get-button -after [get-label X -after [get-label X]] | click
    get-button -after [get-label X 
        -after [get-label "Évaluer le niveau de menace de l'écosystème vis-à-vis de l'objet de l'étude"]] | click
    get-button -after [get-label X -after [get-label X 
        -after [get-label "Identifier et analyser les scénarios de haut niveau, intégrant l'écosystème"]]] | click
    get-button -after [get-label X -after [get-label X -after [get-label X -after [get-label X 
        -after [get-label "Réaliser une étude préliminaire de risque pour identifier les axes prioritaires d'amélioration de la sécurité"]]]]] 
        | click
    get-button -after [get-label X -after [get-label X -after [get-label X -after [get-label X -after [get-label X 
        -after [get-label "Conduire une étude de risque complète et fine, par exemple sur un produit de sécurité ou en vue de l'homologation d'un système"]]]]]] 
        | click
    get-button -after [get-label X -after [get-label X 
        -after [get-label "Orienter un audit de sécurité et notamment un test d'intrusion"]]] | click
    get-button Terminer | click
    with [get-window "Objets nécessaires ajoutés"] {
        get-table | select "Cyber kill chain"
        get-button OK | click
    }
}
with [get-view Projets] {
    click
    get-tree | select "Dépôt local/LeTravail" | double-click
}
