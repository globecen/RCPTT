get-button Exporter | click
with [get-window Exporter] {
    get-tree | select "Bases de connaissances vers un fichier bibliothèque"
    get-button "Suivant >" | click
}
with [get-window "Bases de connaissances"] {
    with [get-section "Bases de connaissances" | get-tree] {
        select "Actions élémentaires"
        get-item -path "Actions élémentaires" | check
        get-item -path "Biens supports" | check
        get-item -path "Catégories d'actions élémentaires" | check
    }
    get-button "Suivant >" | click
}
set-dialog-result File "C:\\ProjetARM\\test.armlibraries"
with [get-window "Bases de connaissances"] {
    get-button "Parcourir..." | click
    get-button Terminer | click
    with [get-window "Objets nécessaires ajoutés"] {
        with [get-table] {
            select "Critères de sécurité" | key-type "" "\\u0000"
            select "Catégories de bien support" | key-type "" "\\u0000"
        }
        get-button OK | click
    }
}
