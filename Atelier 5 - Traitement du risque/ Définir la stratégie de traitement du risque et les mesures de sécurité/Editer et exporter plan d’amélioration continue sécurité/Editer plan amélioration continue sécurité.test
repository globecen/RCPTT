------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view Ateliers | get-canvas | double-click]
{
   mouse-press -x 305 -y 45 -button button1
   mouse-release -x 305 -y 45 -button button1
}
get-view "5 - Traitement du risque" | get-section "Plan d'amélioration continue de la sécurité" 
    | get-button "Afficher le PACS" | click
get-editor PACS | get-section "Plan d'amélioration continue de la sécurité" | get-canvas | double-click
with [get-window Type] {
    get-section "Élément(s) disponible(s)" | get-tree | select "Types de mesure de sécurité/Protection"
    get-button -index 2 | click
    get-button OK | click
}
