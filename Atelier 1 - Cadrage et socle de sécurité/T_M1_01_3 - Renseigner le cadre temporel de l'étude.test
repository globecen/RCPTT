------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view Projets | get-tree | select "Dépôt local/BIOTECH" | double-click
get-view "1 - Cadrage et socle de sécurité" | get-section "Définir les objectifs et le cadre temporel de l'étude" 
    | get-button "Définir les objectifs et le cadre temporel" | click
with [get-editor "Objectifs et cadre temporel de l'étude" | get-section "Cadre temporel de l'étude"] {
    with [get-editbox -after [get-label "Durée des cycles stratégiques :"]] {
        set-text "5 ans"
        set-text "4 ans"
    }
    with [get-editbox -after [get-label "Durée des cycles opérationnels :"]] {
        set-text "2 ans"
        set-text "1 ans"
    }
}
