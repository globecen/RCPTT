get-button Importer | click
with [get-window Importer] {
    get-tree | select "Référentiels de sécurité standard"
    get-button "Suivant >" | click
}
with [get-window "Référentiels de sécurité"] {
    with [get-section "Bases de connaissances" | get-tree] {
        select "ANSI\\/ISA\\/C 62443-3-3 \\(Anglais\\)"
        get-item -path "ANSI\\/ISA\\/C 62443-3-3 \\(Anglais\\)" | check
        select "ANSI\\/ISA\\/C 62443-3-3 \\(Anglais\\)/Mesures de sécurité \\(ANSI\\/ISA\\/CEI 62443-3-3\\)"
    }
    get-button Terminer | click
    with [get-window "Objets nécessaires ajoutés"] {
        get-table | select "Types de référentiel"
        get-button OK | click
    }
}
